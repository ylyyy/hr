
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>profession_design</title>
    <script src="~/javascript/comm/comm.js"></script>
    <link href="~/css/table.css" rel="stylesheet" />
    <script src="~/javascript/jquery-1.7.2.js"></script>
    <script type="text/javascript">
        $(function () {
            $.post("/client/profession_designIndex", "", function (data) {
                var datas = eval("(" + data + ")");
                var $table = $("#tb1");
                //$("#tb1 tr:gt(0)").remove();
                //ftk_id, first_kind_id, first_kind_name, second_kind_id, second_kind_name, third_kind_id, third_kind_name, third_kind_sale_id, third_kind_is_retail
                for (var i = 0; i < datas.length; i++) {
                    var $tr = $("<tr></tr>");
                    var $td = $("<td>" + datas[i].attribute_name + "</td>");
                    var $td1 = $("<td><a href='/client/profession_designDelete/" + datas[i].pbc_id + "'>删除</a></td>");
                    $tr.append($td, $td1);
                    $table.append($tr);
                };
            });
        });
    </script>
</head>
<body>
    <div> 
        <table width="100%">
            <tr>
                <td>
                    <font color="black">
                        您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--职称设置
                    </font>
                </td>
            </tr>
            <tr>
                <td align="right"></td>
            </tr>
        </table>
        <table width="100%" border="1" cellpadding=0 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE1" id="tb1">
            <tr>
                <td width="80%" class="TD_STYLE1">
                    职称名称
                </td>
                <td width="20%" class="TD_STYLE1" align="center">
                    删除
                </td>
            </tr>
        </table>
    </div>
</body>
</html>
