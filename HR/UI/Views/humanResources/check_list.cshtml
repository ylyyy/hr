
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>check_list</title>
    <script src="~/javascript/jquery-1.7.2.js"></script>
    <link rel="stylesheet" href="../../css/table.css" type="text/css" />
</head>
<body>
    <table width="100%">
        <tr>
            <td>
                <font color="black">您正在做的业务是：人力资源--人力资源档案管理--人力资源档案登记复核 </font>
            </td>
        </tr>
        <tr>
            @{List<human_file> lis = ViewData["hm"] as List<human_file>;}
            <td>
                当前等待复核的人力资源档案总数：@lis.Count  例
            </td>
        </tr>
    </table>
    <table width="100%" border="1" cellpadding=5 cellspacing=1
           bordercolorlight=#848284 bordercolordark=#eeeeee
           class="TABLE_STYLE1">
        <tr class="TR_STYLE1">
            <td width="13%" class="TD_STYLE1">
                档案编号
            </td>
            <td width="13%" class="TD_STYLE1">
                姓名
            </td>
            <td width="11%" class="TD_STYLE1">
                性别
            </td>
            <td width="14%" class="TD_STYLE1">
                I级机构
            </td>
            <td width="14%" class="TD_STYLE1">
                II级机构
            </td>
            <td width="14%" class="TD_STYLE1">
                III级机构
            </td>
            <td width="14%" class="TD_STYLE1">
                职称
            </td>
            <td width="7%" class="TD_STYLE1">
                复核
            </td>
        </tr>
        @foreach (human_file item in ViewData["hm"] as List<human_file>)
            {
            <tr>
                <td class="TD_STYLE2">
                    @item.human_id
                </td>
                <td class="TD_STYLE2">
                    @item.human_name
                </td>
                <td class="TD_STYLE2">
                    @item.human_sex
                </td>
                <td class="TD_STYLE2">
                    @item.first_kind_name
                </td>
                <td class="TD_STYLE2">
                    @item.second_kind_name
                </td>
                <td class="TD_STYLE2">
                    @item.third_kind_name
                </td>
                <td class="TD_STYLE2">
                    @item.human_pro_designation
                </td>
                <td class="TD_STYLE2">
                    <a href="/humanResources/human_check?hmid=@item.human_id">复核</a>
                </td>
            </tr>
        }
    </table>
</body>
</html>
