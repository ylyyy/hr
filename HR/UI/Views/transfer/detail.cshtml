@model Entity.major_change

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>

    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <link rel="stylesheet" href="../../css/table.css"
          type="text/css"></link>
</head>

<body>

    <table width="100%">
        <tr>
            <td>
                <font color="black">您正在做的业务是:人力资源--培训管理--调动登记查看</font>
            </td>
        </tr>
        <tr>
            <td align="right">
                <input type="button" class="BUTTON_STYLE1" onclick="javascript:window.history.back();" value="返回">
            </td>
        </tr>
    </table>
    <table width="100%" border="1" cellpadding=0 cellspacing=1
           bordercolorlight=#848284 bordercolordark=#eeeeee
           class="TABLE_STYLE1">
        <tr height="21">
            <td class="TD_STYLE1" width="8%">
                档案编号
            </td>
            <td  width="10%">
                @Model.human_id
            </td>
            <td class="TD_STYLE1" width="8%">
                姓名
            </td>
            <td width="10%">
                @Model.human_name
            </td>
            <td class="TD_STYLE1" width="8%"></td>
            <td class="TD_STYLE2" width="10%"></td>
            <td class="TD_STYLE1" width="8%"></td>
            <td class="TD_STYLE2" width="10%"></td>
        </tr>
        <tr>
            <td class="TD_STYLE1" width="8%">
                原一级机构
            </td>
            <td>
                @Model.first_kind_name
            </td>
            <td class="TD_STYLE1" width="8%">
                原二级机构
            </td>
            <td  width="10%">
                @Model.second_kind_name
            </td>
            <td class="TD_STYLE1" width="8%">
                原三级机构
            </td>
            <td  width="10%">
                @Model.third_kind_name
            </td>
            <td class="TD_STYLE1" width="8%"></td>
            <td class="TD_STYLE2" width="10%"></td>
        </tr>

        <tr>
            <td class="TD_STYLE1">
                原职位分类
            </td>
            <td >
                @Model.major_kind_name
            </td>
            <td class="TD_STYLE1" width="12%">
                原职位名称
            </td>
            <td>
                @Model.major_name
            </td>
            <td class="TD_STYLE1">
                原薪酬标准
            </td>
            <td>
                @Model.salary_standard_name
            </td>
            <td class="TD_STYLE1">
                @Model.salary_sum
            </td>
            <td></td>
        </tr>
        <tr>
            <td class="TD_STYLE1">
                新一级机构名称
            </td>
            <td>
                @Model.new_first_kind_name
            </td>
            <td class="TD_STYLE1" width="12%">
                新二级机构名称
            </td>
            <td>
                @Model.new_second_kind_name
            </td>
            <td class="TD_STYLE1" width="12%">
                新三级机构名称
            </td>
            <td>
                @Model.new_third_kind_name
            </td>
            <td class="TD_STYLE1" width="8%"></td>
            <td class="TD_STYLE2" width="10%"></td>
        </tr>
        <tr>
            <td class="TD_STYLE1">
                新职位分类
            </td>
            <td>
                @Model.new_major_kind_name
            </td>
            <td class="TD_STYLE1" width="12%">
                新职位名称
            </td>
            <td>
                @Model.new_major_name
            </td>
            <td class="TD_STYLE1">
                新薪酬标准
            </td>
            <td>
                @Model.new_salary_standard_name
            </td>
            <td class="TD_STYLE1">
                新薪酬总额
            </td>
            <td>
               @Model.new_salary_sum
            </td>
        </tr>

        <tr>
            <td class="TD_STYLE1">
                登记人
            </td>
            <td>
               @Model.register
            </td>
            <td class="TD_STYLE1">
                登记时间
            </td>
            <td>
                @Model.regist_time
            </td>
            <td class="TD_STYLE1">
                复核人
            </td>
            <td>
                @Model.checker
            </td>
            <td class="TD_STYLE1">
                复核时间
            </td>
            <td>
                @Model.check_time
            </td>
        </tr>
        <tr>
            <td class="TD_STYLE1">
                调动原因
            </td>
            <td colspan="7" class="TD_STYLE2">
                <textarea name="majorChange.changeReason" rows="6" readonly="readonly" class="TEXTAREA_STYLE1">@Model.check_reason</textarea>
            </td>
        </tr>
    </table>

</body>
</html>
