                    薪酬标准
                </td>
                <td width="19%" class="TD_STYLE1">
                    名字
                </td>
                <td width="5%" class="TD_STYLE1">
                    调动

                </td>

            </tr>
            @foreach (human_file item in Model)
            {
                <tr>
                    <td>  @item.first_kind_name</td>
                    <td>  @item.second_kind_name</td>
                    <td>  @item.third_kind_name</td>
                    <td>  @item.salary_standard_name</td>
                    <td>  @item.salary_sum</td>
                    <td><a href="updateRegister/?id=@item.human_id">调动</a></td>
                </tr>

            }


        </table>


        <html>
