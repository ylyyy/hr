
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>salary_item_register</title>
    <link rel="stylesheet" href="~/css/table.css" type="text/css">
    <script src="~/javascript/jquery-1.7.2.js"></script>
    @*<script type="text/javascript" src="~/javascript/comm/comm.js">*@
    @*</script>*@


    <script type="text/javascript">
        
        
        function check() {
            //alert($("#itemName").val() == "");
            if ($("#itemName").val() == "") {
                alert("薪酬项目名称不能为空!!!");
                return false;
            }
            //document.forms[0].submit();
            return true;
        }

    </script>
</head>
<body>
    <form action="/client/salary_item" method="post">

        <table width="100%">
            <tr>
                <td>
                    <font color="black">
                        您正在做的业务是：人力资源--客户化设置--薪酬管理设这--薪酬项目设置--薪酬项目添加
                    </font>
                </td>
            </tr>
            <tr>
                <td align="right">
                    <input type="submit" value="提交" class="BUTTON_STYLE1" onclick="return check()">
                    <input type="button" value="返回" class="BUTTON_STYLE1"
                           onclick="history.back();">
                </td>
            </tr>
        </table>

        <table width="100%" border="1" cellpadding=0 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE1">
            <tr>
                <td class="TD_STYLE1" width="19%">
                    薪酬项目名称
                </td>
                <td class="TD_STYLE2" width="81%">
                    <input type="text" class="INPUT_STYLE1" id="itemName" name="itemName" />
                </td>
            </tr>
        </table>
    </form>
</body>
</html>
